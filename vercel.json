{
  "version": 2,
  "public": false,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "src/index.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "src/index.js" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "/index.js" }
  ]
}
